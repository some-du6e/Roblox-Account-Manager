{
  "applicationId": "com.neutralino.rblxaccountmanager",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/resources/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "debug.log",
    "filesystem.*",
    "storage.*",
    "computer.*",
    "clipboard.*",
    "updater.*"
  ],
  "modes": {
    "window": {
      "title": "Roblox Account Manager",
      "width": 1200,
      "height": 800,
      "minWidth": 800,
      "minHeight": 600,
      "resizable": true,
      "maximizable": true,
      "minimizable": true,
      "closable": true,
      "alwaysOnTop": false,
      "center": true,
      "fullScreen": false,
      "hidden": false,
      "useSavedState": true,
      "enableInspector": true
    }
  },
  "cli": {
    "binaryName": "rblx-account-manager",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "4.13.0",
    "clientVersion": "3.10.0"
  }
}